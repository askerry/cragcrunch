    			};
    			function getnewuser(){
					bootbox.prompt("Welcome to CragCrunch! Please enter a user name?", gotosetuppage);
				};

			$(document).ready(function(){

				$("#sportcheck").bootstrapSwitch({off:'No',on:'Yes',labelText:'Sport',size: 'sm'});
